<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.hellocordova" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:rim="http://www.blackberry.com/ns/widgets">
    <name>Colorado-Concerts</name>
    <description>
        Colorado Concerts Mobile Application
    </description>
    <author email="jmcplease@gmail.com">
        Jonathan Chu
    </author>
    <content src="index.html" />
    <access origin="*" />
    <feature name="Geolocation" value="Geolocation" />
    <rim:permissions>
        <rim:permit>read_geolocation</rim:permit>
        <rim:permit>access_shared</rim:permit>
    </rim:permissions>
    <feature name="Accelerometer" value="Accelerometer" />
    <feature name="Camera" value="Camera" />
</widget>
